{
  "name": "cdk-cdn-construct",
  "private": true,
  "version": "0.1.0",
  "bin": {
    "cdk-cdn-construct": "bin/cdk-cdn-construct.js"
  },
  "main": "lib/cdk-cdn-construct-stack",
  "scripts": {
    "build": "npm install --no-package-lock --prefix ./api && tsc",
    "watch": "tsc -w",
    "test": "npm run build && jest",
    "deploy": "npm run clear && npm run build && cdk deploy --all --require-approval never",
    "clear": "rimraf cdk.out api/node_modules",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.118.0",
    "@types/jest": "27.0.0",
    "@types/node": "16.4.13",
    "jest": "27.0.6",
    "lambda-local": "2.0.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.2.0",
    "typescript": "4.3.5",
    "aws-cdk": "1.118.0"
  },
  "dependencies": {
    "@aws-cdk/aws-certificatemanager": "1.118.0",
    "@aws-cdk/aws-cloudfront": "1.118.0",
    "@aws-cdk/aws-route53": "1.118.0",
    "@aws-cdk/aws-route53-targets": "1.118.0",
    "@aws-cdk/aws-s3": "1.118.0",
    "@aws-cdk/aws-s3-deployment": "1.118.0"
  },
  "peerDependencies": {
    "@aws-cdk/core": "1.118.0"
  }
}
